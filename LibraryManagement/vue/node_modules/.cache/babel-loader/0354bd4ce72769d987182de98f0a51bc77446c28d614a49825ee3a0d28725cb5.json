      if (!value) {\n                return callback(new Error(\"手机不能为空\"));\n            }\n            if (!/^[1][3,4,5,6,7,8,9][0-9]{9}$/.test(value)) {\n                callback(new Error(\"请输入合法的手机号\"));\n            }\n\n            callback();\n        };\n\n        return {\n            form: {},\n            rules: {\n                name: [{\n                    required: true,\n                    message: \"请输入用户名称\",\n                    trigger: \"blur\"\n                }],\n                age: [{\n                    validator: checkAge,\n                    trigger: \"blur\"\n                }],\n                phone: [{\n                    validator: checkPhone,\n                    trigger: \"blur\"\n                }],\n            },\n        };\n    },\n    methods: {\n    },\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AAuDA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;IACA,IAAAC,QAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,KAAAD,KAAA;QACA,OAAAC,QAAA,KAAAC,KAAA;MACA;MACA,KAAAC,MAAA,CAAAC,SAAA,CAAAJ,KAAA;QACAC,QAAA,KAAAC,KAAA;MACA;MACA,IAAAF,KAAA,WAAAA,KAAA;QACAC,QAAA,KAAAC,KAAA;MACA;MACAD,QAAA;IACA;IACA;IACA,IAAAI,UAAA,GAAAA,CAAAN,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,KAAAD,KAAA;QACA,OAAAC,QAAA,KAAAC,KAAA;MACA;MACA,oCAAAI,IAAA,CAAAN,KAAA;QACAC,QAAA,KAAAC,KAAA;MACA;MAEAD,QAAA;IACA;IAEA;MACAM,IAAA;MACAC,KAAA;QACAZ,IAAA;UACAa,QAAA;UACAC,OAAA;UACAC,OAAA;QACA;QACAC,GAAA;UACAC,SAAA,EAAAf,QAAA;UACAa,OAAA;QACA;QACAG,KAAA;UACAD,SAAA,EAAAR,UAAA;UACAM,OAAA;QACA;MACA;IACA;EACA;EACAI,OAAA,GACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}